}

.empty-block {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  height: 100%;
  padding: 0 20px;
  margin-top: 32px;
  color: rgba(0, 0, 0, 0.3);
}

.empty-block .icon-box-ctn {
  margin-left: 4px;
}
