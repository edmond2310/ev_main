/**index.wxss**/
page {
  background-color: #f8f8f8;
  color: #555555;
}

.container-bill {
  padding-bottom: 88px;
}

.bill-item {
  margin-bottom: 12px;
}

.weui-form-preview__bd {
  line-height: 1.65;
}

.weui-form-preview__hd {
  padding-top: 8px;
  padding-bottom: 8px;
}

.finish {
  color: rgb(7, 193, 96);
}

.finish .weui-form-preview__label,
.finish .weui-form-preview__value {
